<!-- This file takes variables sent to the ejs and makes them usable in the javascript file. -->
<%if(user){%>
    <script>
        let user = JSON.parse('<%-JSON.stringify(user)%>'); 
    </script>
<%}else{%>
    <script>
        let user = false;
    </script>
<%}%>
<%if(sentState){%>
    <script>
        let sentState = JSON.parse('<%-JSON.stringify(sentState)%>');
    </script>
<%}else{%>
    <script>
        let sentState = false;
    </script>
<%}%>
<%if(game){%>
    <script>
        let game = JSON.parse('<%-JSON.stringify(game)%>');
    </script>
<%}else{%>
    <script>
        let game = {};
    </script>
<%}%>